Host github.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa

Host personal.github.com
    HostName github.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa_personal_yubikey.pub

Host polychain.github.com
    HostName github.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa_personal_yubikey.pub


Host polychain.gitlab.com
    HostName gitlab.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa_pc_yubikey.pub

Host personal.gitlab.com
    HostName gitlab.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa_personal_yubikey.pub
